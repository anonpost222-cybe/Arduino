{
  "version": 1,
  "author": "Smart Warehouse",
  "editor": "wokwi",
  "firmware": ".pio/build/esp32doit-devkit-v1/firmware.bin",
  "elf": ".pio/build/esp32doit-devkit-v1/firmware.elf",
  "parts": [
    { "type": "board-esp32-devkit-c-v4", "id": "esp", "top": 0, "left": 0, "attrs": {} },
    { "type": "wokwi-dht22", "id": "dht1", "top": -80, "left": 120, "attrs": {} },
    { "type": "wokwi-potentiometer", "id": "potSmoke", "top": 100, "left": -60, "attrs": { "label": "Smoke Level (MQ-2)" } },
    { "type": "wokwi-relay-module", "id": "relay1", "top": 80, "left": 150, "attrs": {} },
    { "type": "wokwi-led", "id": "ledVent", "top": 80, "left": 260, "attrs": { "color": "cyan", "label": "FAN ON" } },
    { "type": "wokwi-resistor", "id": "r1", "top": 150, "left": 260, "attrs": { "value": "220" } }
  ],
  "connections": [
    [ "esp:TX", "$serialMonitor:RX", "", [] ],
    [ "esp:RX", "$serialMonitor:TX", "", [] ],
    
    [ "dht1:VCC", "esp:3V3", "red", [ "v0" ] ],
    [ "dht1:GND", "esp:GND.1", "black", [ "v0" ] ],
    [ "dht1:SDA", "esp:15", "green", [ "v0" ] ],

    [ "potSmoke:VCC", "esp:3V3", "red", [ "h-20", "v-50" ] ],
    [ "potSmoke:GND", "esp:GND.2", "black", [ "h-20", "v-50" ] ],
    [ "potSmoke:SIG", "esp:34", "orange", [ "h20", "v-50" ] ],

    [ "relay1:VCC", "esp:5V", "red", [ "v0" ] ],
    [ "relay1:GND", "esp:GND.3", "black", [ "v0" ] ],
    [ "relay1:IN", "esp:2", "blue", [ "v0" ] ],

    [ "relay1:NO", "ledVent:A", "red", [ "h0" ] ],
    [ "relay1:COM", "esp:5V", "red", [ "h-20", "v40" ] ],
    
    [ "ledVent:C", "r1:1", "black", [ "v0" ] ],
    [ "r1:2", "esp:GND.3", "black", [ "h-20", "v50" ] ]
  ],
  "dependencies": {}
}