{
  "version": 1,
  "author": "Smart Queue",
  "editor": "wokwi",
  "parts": [
    { "type": "board-esp32-devkit-c-v4", "id": "esp", "top": 0, "left": 0, "attrs": {} },
    { "type": "board-ssd1306", "id": "oled", "top": -80, "left": 50, "attrs": { "i2cAddress": "0x3c" } },
    { "type": "wokwi-pushbutton", "id": "btnNew", "top": 100, "left": -60, "attrs": { "color": "green", "label": "New Customer" } },
    { "type": "wokwi-pushbutton", "id": "btnDone", "top": 100, "left": 60, "attrs": { "color": "red", "label": "Served" } },
    { "type": "wokwi-potentiometer", "id": "potSpeed", "top": 100, "left": 180, "attrs": { "label": "Avg Service Time (min)" } }
  ],
  "connections": [
    [ "esp:TX", "$serialMonitor:RX", "", [] ],
    [ "esp:RX", "$serialMonitor:TX", "", [] ],
    [ "oled:VCC", "esp:3V3", "red", [ "v0" ] ],
    [ "oled:GND", "esp:GND.1", "black", [ "v0" ] ],
    [ "oled:SDA", "esp:21", "purple", [ "v0" ] ],
    [ "oled:SCL", "esp:22", "cyan", [ "v0" ] ],
    [ "btnNew:2.l", "esp:GND.2", "black", [ "h0" ] ],
    [ "btnNew:1.l", "esp:13", "green", [ "h0" ] ],
    [ "btnDone:2.l", "esp:GND.2", "black", [ "h0" ] ],
    [ "btnDone:1.l", "esp:12", "red", [ "h0" ] ],
    [ "potSpeed:VCC", "esp:3V3", "red", [ "h-20", "v-50" ] ],
    [ "potSpeed:GND", "esp:GND.3", "black", [ "h-20", "v-50" ] ],
    [ "potSpeed:SIG", "esp:34", "blue", [ "h20", "v-50" ] ]
  ],
  "dependencies": {}
}