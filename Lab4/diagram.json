{
  "version": 1,
  "author": "AI Engineer",
  "editor": "wokwi",
  "firmware": ".pio/build/esp32doit-devkit-v1/firmware.bin",
  "elf": ".pio/build/esp32doit-devkit-v1/firmware.elf",
  "parts": [
    { "type": "board-esp32-devkit-c-v4", "id": "esp", "top": 0, "left": 0, "attrs": {} },
    { "type": "wokwi-dht22", "id": "dht1", "top": -80, "left": 60, "attrs": {} },
    { "type": "wokwi-servo", "id": "servo1", "top": -80, "left": 180, "attrs": {} },
    { "type": "wokwi-photoresistor-sensor", "id": "ldr1", "top": 120, "left": -100, "attrs": {} },
    { "type": "wokwi-led", "id": "led1", "top": 50, "left": 150, "attrs": { "color": "blue", "label": "Pump" } },
    { "type": "wokwi-resistor", "id": "r1", "top": 100, "left": 150, "attrs": { "value": "220" } }
  ],
  "connections": [
    [ "esp:TX", "$serialMonitor:RX", "", [] ],
    [ "esp:RX", "$serialMonitor:TX", "", [] ],
    
    [ "dht1:VCC", "esp:3V3", "red", [ "v0" ] ],
    [ "dht1:GND", "esp:GND.1", "black", [ "v0" ] ],
    [ "dht1:SDA", "esp:15", "orange", [ "v0" ] ],

    [ "servo1:V+", "esp:5V", "red", [ "v0" ] ],
    [ "servo1:GND", "esp:GND.2", "black", [ "v0" ] ],
    [ "servo1:PWM", "esp:13", "green", [ "v0" ] ],

    [ "ldr1:VCC", "esp:3V3", "red", [ "h-20", "v-100" ] ],
    [ "ldr1:GND", "esp:GND.3", "black", [ "h0" ] ],
    [ "ldr1:AO", "esp:34", "purple", [ "h0" ] ],

    [ "led1:A", "esp:2", "blue", [ "v0" ] ],
    [ "led1:C", "r1:1", "black", [ "v0" ] ],
    [ "r1:2", "esp:GND.2", "black", [ "v0" ] ]
  ],
  "dependencies": {}
}